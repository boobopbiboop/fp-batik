generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Kain {
  id          Int      @id @default(autoincrement())
  name        String   @unique
  class       String
  description String
  origin      String
  century     Int
  type        String
  meaning     String
  pilosophy   String
  dirUrl      String
  history     String
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  @@map("kain")
}
